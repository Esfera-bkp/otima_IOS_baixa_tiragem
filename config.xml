<?xml version='1.0' encoding='utf-8'?>
<widget id="otima.baixatiragem.phonegap" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Baixa tiragem</name>
    <description>
        App baixa tiragem Ótima gráfica
    </description>
    <author email="chao@londigital.com.br" href="http://londigital.com.br">
        l.on digital
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="orientation" value="landscape" />
    <preference name="BackupWebStorage" value="local" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <engine name="ios" spec="^4.5.4" />
    <plugin name="cordova-plugin-file" spec="^4.3.3" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.6.3" />
    <plugin name="cordova-plugin-device" spec="^1.1.6" />
    <plugin name="cordova-sqlite-storage" spec="^2.0.4" />
    <plugin name="cordova-plugin-dialogs" spec="^1.3.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.2" />
</widget>
